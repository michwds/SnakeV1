<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake"
             StartupUri="Window.xaml">
    <ApplicationResources>
        <SolidColorBrush x:Key="BackgroundColor"></SolidColorBrush>
        <SolidColorBrush x:Key="PlayAreaColor"></SolidColorBrush>
        <SolidColorBrush x:Key="TecxtColor">#EBEBE</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
    </ApplicationResources>
</Application>